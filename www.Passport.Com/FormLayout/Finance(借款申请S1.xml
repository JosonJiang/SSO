    <Date Type="String" Name="预计还款日期"></Date>
    <Pay_Name Type="String" Name="领款人"></Pay_Name>
    <Bank_Personal Type="String" Name="领款人账号"></Bank_Personal>
    <Reason Type="String" Name="借款事由"></Reason>
    <Cash_No Type="String" Name="付款银行"></Cash_No>
